{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"moduleResolution": "node16",
		"paths": {
			"styles": ["styles/index"],
			"@/*": ["./*"],
			"react-native-maps": ["node_modules/react-native-maps"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
